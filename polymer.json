{
  "entrypoint": "index.html",
  "componentDir": "elements/",
  "shell": "index.js",
  "sources": [
    "elements/*.css"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/bundles/*.js",
    "node_modules/@webcomponents/webcomponentsjs/*.js"
  ],
  "builds": [{
    "name": "prod",
    "browserCapabilities": [
      "es2015"
    ],
    "html": {
      "minify": true
    },
    "css": {
      "minify": true
    },
    "js": {
      "minify": true
    },
    "bundle": true,
    "addServiceWorker": false
  }]
}